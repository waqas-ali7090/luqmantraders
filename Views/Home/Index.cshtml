<!--PORTFOLIO SECTION -->
<section id="portfolio-section">
    <div class="wrap-pad">
        <div class="row">
            <div class="col-md-8 col-md-offset-2 col-sm-10 col-sm-offset-1 " data-scrollreveal="enter top and move 100px, wait 0.5s">
                <div class="text-center">
                    <h1>
                        <i class="fa fa-star-o small-icons bk-color-light-blue"></i>Products
                    </h1>
                    <p class="lead">
                        These are the products available for our customers, get your deal now. We are not limited to only these products,
                        We can provide products on order, according to your desire.
                    </p>
                </div>
            </div>
            <!-- ./ Heading div-->

            <div class="col-md-10 col-md-offset-1 col-sm-12" data-scrollreveal="enter right and move 100px, wait 0.6s">
                <ul class="portfolio-items">

                    @foreach (var item in Model.Items)
                    {
                        <li class="portfolio-item ">
                            <div class="item-main">
                                <div class="portfolio-image">
                                    <img src="@Url.Content(item.Photo)" alt="Loading Image" style="width: 240px; height: 245px" />
                                    <div class="overlay">
                                        <a class="preview btn btn-primary" title="@item.Name" href="@Url.Content(item.Photo)"><i class="fa fa-eye"></i></a>
                                    </div>
                                </div>
                                @if (item.Description != null)
                                {
                                    <h5>@item.Description</h5>
                                }
                                else
                                {
                                    <span></span>
                                }
                            </div>
                        </li>
                    }
                </ul>
            </div>
            <!-- ./ Content div-->
        </div>
    </div>
</section>
<!--PORTFOLIO SECTION -->
